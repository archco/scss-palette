{
  "name": "scss-palette",
  "version": "0.3.3",
  "description": "The color palette for SCSS.",
  "main": "src/_palette.scss",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "node banner.js",
    "build": "npm run sass && npm run json",
    "sass": "node node_modules/node-sass/bin/node-sass tests/test.scss tests/test.css",
    "sass:watch": "npm run sass && npm run sass -- --watch",
    "json": "node node_modules/node-sass/bin/node-sass tests/colors.scss tests/colors.css",
    "postjson": "node tests/convert-to-json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archco/scss-palette.git"
  },
  "author": "archco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/archco/scss-palette/issues"
  },
  "homepage": "https://github.com/archco/scss-palette#readme",
  "devDependencies": {
    "node-sass": "^4.5.0",
    "replace-in-file": "^2.4.0"
  }
}
